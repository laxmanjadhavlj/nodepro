{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/products",
            "destination": "index.js"
        },
        {
            "src": "/products/(.*)",
            "destination": "index.js"
        },
        {
            "src": "/",
            "destination": "build/index.html"
        },
        {
            "src": "/(.*)",
            "destination": "build/$1"
        }
    ]
}